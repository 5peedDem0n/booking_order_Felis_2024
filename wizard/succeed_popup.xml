<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="success_popup_form" model="ir.ui.view">
        <field name="name">success.popup.form</field>
        <field name="model">booking_order.success_popup</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="message"/>
                </group>
                <footer>
                    <button string="Close" class="btn-default" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
